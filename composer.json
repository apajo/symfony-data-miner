{
    "name": "apajo/symfony-data-miner",
    "type": "symfony-bundle",
    "description": "Symfony bundle for php-data-miner",
    "keywords": ["symfony", "ml", "data", "miner"],
    "homepage": "https://github.com/apajo/symfony-data-miner",
    "license": "MIT",
    "authors": [
        {
            "name": "Andres Pajo",
            "homepage": "http://apajo.ee/"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/apajo/php-data-miner.git"
        }
    ],
    "require": {
        "php": "^7.4|^8.1",
        "apajo/php-data-miner": "^0.2.15"
    },
    "autoload": {
        "psr-4": {
            "DataMiner\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhpDataMiner\\": "../php-data-miner/src/"
        }
    }
}
